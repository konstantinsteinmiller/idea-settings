<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JSON" injector-id="xml">
      <display-name>i18n</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("i18n")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>